/*Program To Convert Centigrade To Fahrenhit*/
#include<stdio.h>
#include<conio.h>
void main() //execution start from here i.every C Program have one function
{
  float Centigrade,Fahrenhit;
  clrscr();  	 //Clear  The Console Screen
  printf("Enter The Temperature In Centigrade To Convert Into Fahrenhit:-");
  scanf("%f",&Centigrade);
  Fahrenhit=(9/5.0*Centigrade)+32;
  printf("The degree In Fahrenhit Is:%f",Fahrenhit);
  getch();
}